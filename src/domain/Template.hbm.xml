<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.Template" table="TEMPLATE">
		<id name="id">
			<column name="ID_TEMPLATE" />
			<generator class="increment" />
		</id>

		<property name="url">
			<column name="URL" />
		</property>
		<property name="nom">
			<column name="NOM" />
		</property>

	</class>
</hibernate-mapping>