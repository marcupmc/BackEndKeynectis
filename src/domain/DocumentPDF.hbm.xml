<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.DocumentPDF" table="DOCUMENTPDF">
	    <id name="id">
			<column name="ID_DOCUMENT" />
			<generator class="increment" />
		</id>
		<property name="name">
			<column name="NAME" />
		</property>
		<property name="url">
			<column name="URL" />
		</property>
	    <property name="certified" type="domain.MyBooleanType">
			<column name="CERTIFIED" />
		</property>
		<property name="signatureName">
			<column name="SIGNAME" />
		</property>
		<property name="signatureX">
			<column name="SIGX" />
		</property>
		<property name="signatureY">
			<column name="SIGY" />
		</property>
		
	     <many-to-one name="owner" column="ID_UTILISATEUR" lazy="false" class="domain.Utilisateur"  unique="false" />
		
	</class>
</hibernate-mapping>