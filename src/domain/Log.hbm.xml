<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.Log" table="LOG">
		<id name="id">
			<column name="ID_LOG" />
			<generator class="increment" />
		</id>

		<property name="type">
			<column name="TYPE" />
		</property>
		<property name="date">
			<column name="DATE" />
		</property>
		<property name="ipadresse">
			<column name="IP" />
		</property>

		<property name="identifiant_client">
			<column name="ID_CLIENT" />
		</property>
		<property name="eventype">
			<column name="EVENT" />
		</property>
		<property name="latitude">
			<column name="LATITUDE" />
		</property>
		<property name="longitude">
			<column name="LONGITUDE" />
		</property>

	</class>
</hibernate-mapping>
